pipeline {

export M2_HOME=/home/nilesh/Programs/maven/apache-maven-3.3.9
export PATH=$PATH:$M2_HOME/bin

	agent any

	stages {
		
		stage('Compile Stage') {
			steps {
					sh 'mvn clean compile'
				
			}
		}
		
		stage('Testing Stage') {
			steps {
					sh 'mvn test'
			}
		}
		
		stage('Deployment Stage') {
			steps {
					//sh 'mvn deploy'
					echo 'Deployment is going on'
			}
		}
		

	}
	
}